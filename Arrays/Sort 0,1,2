/*




*/


public class Solution{
    public static void sort012( int [] arr, int n){
        int low=0;
        int mid=0;
        int high=n;
        int temp;
    while( mid <= high){
        if(arr[mid] == 0){
           temp= arr[low];
           arr[low]=arr[mid];
           arr[mid]=temp;
            low++;
            mid++;
      }else if(arr[mid] == 1){
        mid++;
} else{
     temp=arr[high];
     arr[high]=arr[mid];
     arr[mid]=temp;
     high--;
}
}
}
}












    
